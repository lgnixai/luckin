{
  "name": "@lgnixai/luckin-foundation",
  "version": "3.0.0",
  "description": "Core foundation infrastructure for Luckin IDE",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rimraf dist",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "luckin",
    "ide",
    "foundation",
    "dependency-injection",
    "event-system"
  ],
  "author": "LGINX AI Corporation",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "rimraf": "^5.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.3.0"
  },
  "peerDependencies": {
    "typescript": ">=5.0.0"
  }
}